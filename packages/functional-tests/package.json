  "scripts": {
    "clean": "tsc --build --clean",
    "build": "tsc --build && eslint --ext .ts src",
    "build-only": "tsc --build",
    "build-watch": "tsc --build --watch --preserveWatchOutput",
    "start": "node .",
    "start-watch": "nodemon --nolazy --inspect .",
