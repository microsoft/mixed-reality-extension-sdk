{
  "extends": "./tslint.json",
  "rules": {
    "completed-docs": [
      true,
      {
        "enums": {
          "visibilities": [
            "exported"
          ],
          "tags": {
            "content": {},
            "existence": [
              "hidden"
            ]
          }
        },
        "types": {
          "visibilities": [
            "exported"
          ],
          "tags": {
            "content": {},
            "existence": [
              "hidden"
            ]
          }
        },
        "functions": {
          "visibilities": [
            "exported"
          ],
          "tags": {
            "content": {},
            "existence": [
              "hidden"
            ]
          }
        },
        "classes": {
          "visibilities": [
            "exported"
          ],
          "tags": {
            "content": {},
            "existence": [
              "hidden"
            ]
          }
        },
        "visibilities": {
          "visibilities": [
            "exported"
          ],
          "tags": {
            "content": {},
            "existence": [
              "hidden"
            ]
          }
        },
        "methods": {
          "locations": "all",
          "privacies": [
            "public"
          ],
          "tags": {
            "content": {},
            "existence": [
              "hidden"
            ]
          }
        },
        "properties": {
          "locations": "all",
          "privacies": [
            "public"
          ],
          "tags": {
            "content": {
              "see": [
                "#.*"
              ]
            },
            "existence": [
              "inheritdoc"
            ]
          }
        }
      }
    ]
  }
}